HOST=0.0.0.0
PORT=8765
AUTH_TOKEN=change_me

# Client-friendly address (mDNS). Example: "mac-mini.local"
# ESP32 can use this instead of scanning for an IP.
MDNS_HOSTNAME=jiabos-macbook-pro-2.local

WHISPER_BIN=/ABS/PATH/TO/whisper.cpp/main
WHISPER_MODEL=/ABS/PATH/TO/models/ggml-medium.bin
# Example: --language zh --temperature 0
WHISPER_ARGS=--language zh --temperature 0

DEFAULT_MODE=return_only
MAX_AUDIO_SEC=30
SAMPLE_RATE=16000
CHANNELS=1
BIT_DEPTH=16
KEEP_DEBUG=false
